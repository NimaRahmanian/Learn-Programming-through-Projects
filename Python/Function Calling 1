def calcClassic(carDate, classicYear):
    yrs = classicYear - carDate
    determineClassic(yrs)
  
def determineClassic(yrs):
    if (yrs > 20):
      print("Congratulations, your car is classic")
    else:
      print("Your car is not a classic yet")

carYear = int(input("Enter the year your car was built in: "))
classic = int(input("What is the classic year: "))
calcClassic(carYear, classic)
