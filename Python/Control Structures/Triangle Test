s1 = float(input("What is s1: "))
s2 = float(input("What is s2: "))
s3 = float(input("What is s3: "))

if ( s1 <= 0 or s2 <= 0 or s3 <= 0 ):
	print("Positive, non negative numbers only")
else:
	if ( ((s1 + s2) > s3 ) and ( (s1 + s2) > s3 ) and ( (s2 + s3) > s1) ):
		if ( s1 == s2 and s1 == s3 and s2 == s3 ):
			print("Equilateral")
		elif ( s1 == s2 or s1 == s3 or s2 == s3 ):
			print("Isosceles")
		else:
			print("Scalene")
	else:
		print("Sides do not make a triangle!")
