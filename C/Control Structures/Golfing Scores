#include<stdio.h>

int main() {
  //str is stroke
	int str;

	printf("Number of strokes: ");
	scanf("%d", &str);

  //nested if--else if--else conditions
  //general format
  //if(condition){}
  //else if (condition){}
  //else{}
	if (str > 0){
		if (str == 1){ 
			printf("hole in one"); 
		} else if (str == 2){ 
			printf("double eagle"); 
		}else if (str == 3){ 
			printf("eagle"); 
		} else if (str == 4){ 
			printf("birdie"); 
		} else if (str == 5){ 
		  printf("par"); 
		} else if (str == 6){ 
		  printf("bogey"); 
		} else if (str == 7){ 
		  printf("double bogey"); 
		} else if (str == 8){ 
		  printf("triple bogey"); 
		} else { 
		  printf("disaster"); 
		}
	} else{
		printf("Invalid input");
	}
}

