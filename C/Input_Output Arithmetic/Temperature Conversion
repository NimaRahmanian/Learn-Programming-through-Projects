#include <stdio.h>

int main() {
  double fahrenheit;
  double celsius;
    
  printf("Enter degrees in Fahrenheit: ");
  scanf("%lf", &fahrenheit);
  //order of precedence is imperative
  celsius = (fahrenheit - 32) * (5.0/9.0);
  printf("The equivalent in Celsius is %lf \n", celsius);
  printf("Enter degrees in Celsius: ");
  scanf("%lf", &celsius);
  fahrenheit = (celsius * 9.0/5.0) + 32;
  printf("The equivalent in Fahrenheit is %lf\n", fahrenheit);
}
